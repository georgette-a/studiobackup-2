word\n        \n        auth.signInWithEmailAndPassword(email, password)\n            .then(cred => {\n              console.log(cred.user)\n              this.$router.push('/dashboard')\n        \n            }).catch(error => {alert(error.Message)})\n        \n      },\n    \n\n      }\n}\n\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n\n\n</style>"],"sourceRoot":""}]}