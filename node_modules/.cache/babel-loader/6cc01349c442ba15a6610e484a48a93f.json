    <tbody class=\"bg-white divide-y divide-gray-200\">\n            <tr v-for=\"client in clients\" v-bind:key=\"client.id\">\n              <td class=\"px-6 py-4 whitespace-nowrap\">\n                <div class=\"flex items-center\">\n                  \n                  <div class=\"ml-4\">\n                    <div class=\"text-sm font-medium text-gray-900\">\n                      {{client.name}}\n                    </div>\n                    \n                  </div>\n                </div>\n              </td>\n              <td class=\"px-6 py-4 whitespace-nowrap\">\n                <div class=\"text-sm text-gray-900\">{{client.email}}</div>\n  \n              </td>\n              \n              <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">\n                {{client.projects}}\n              </td>\n              <td class=\"px-6 py-4 whitespace-nowrap text-right text-sm font-medium\">\n                <router-link to=\"clientlist/editclient\" tag=\"button\" class=\"text-indigo-600 hover:text-indigo-900 bg-transparent\">Edit</router-link>\n              </td>\n            </tr>\n\n            <!-- More items... -->\n          </tbody>\n        </table>\n      </div>\n    </div>\n  </div>\n</div>\n</div>\n</div>\n\n</div>\n\n</template>\n<script>\n\nimport 'firebase/auth'\nimport db from \"./firebaseInit\"\n\n // eslint-disable-next-line no-unused-vars\nvar usersCollection = db.collection(\"Users\");\nexport default {\n      watch: {\n    $route: {\n      immediate: true,\n      handler: function(newVal) {\n        this.showModal = newVal.meta && newVal.meta.showModal;\n      }\n}\n  },\n   \n  data() {\n    return{\n      clients:[]\n    }\n    \n  },\n\n  created () {\n     this.getClients();\n    },\n\n  methods:{\n    getClients(){\n        usersCollection.get().then((querySnapshot) => {\n      this.loading = false\n      querySnapshot.forEach((doc) => {\n        if (doc.data().admin == false){\n        const data = {\n          \n            'name': doc.data().Name,\n            'email': doc.data().email,\n            'projects': (doc.data().Projects).toString()\n\n          }\n          console.log(data)\n          this.clients.push(data)\n        }\n        \n      })\n    })\n    },\n    fetchData () {\n      usersCollection.where('userID', '==', this.$route.params.userID).get().then((querySnapshot) => {\n        querySnapshot.forEach((doc) => {\n          this.userID = doc.id\n          this.userName = doc.data().name\n          this.userEmail = doc.data().price\n        })\n      })\n    },\n  }\n}\n</script>"],"sourceRoot":""}]}