n border-opacity-20\">\n            <dl>\n            <div class=\"bg-sf-green bg-opacity-10 px-4 py-5 sm:grid sm:grid-cols-3 sm:gap-4 sm:px-6\">\n                <dt class=\"text-sm font-medium text-gray-500\">\n                Name\n                </dt>\n                <dd class=\"mt-1 text-sm text-gray-900 sm:mt-0 sm:col-span-2\">\n                {{mealName}}\n                </dd>\n            </div>\n            <div class=\"bg-white px-4 py-5 sm:grid sm:grid-cols-3 sm:gap-4 sm:px-6\">\n                <dt class=\"text-sm font-medium text-gray-500\">\n                Price\n                </dt>\n                <dd class=\"mt-1 text-sm text-gray-900 sm:mt-0 sm:col-span-2\">\n                {{mealPrice}}\n                </dd>\n            </div>\n            \n            </dl>\n        </div>\n        </div>        \n    </div>\n    <div class=\"mt-5 pt-8 md:mt-0 md:col-span-2\">\n      <h1>Edit {{mealName}} </h1>\n      <form>\n        \n        <div class=\"shadow overflow-hidden sm:rounded-md\">\n          <div class=\"px-4 py-5 bg-white sm:p-6\">\n            \n            <div class=\"grid grid-cols-6 gap-6\">\n              \n              <div class=\"col-span-6 sm:col-span-3\">\n                <label for=\"meal_name\" class=\"block text-sm font-medium text-gray-700\">Meal Name</label>\n                <input type=\"text\" name=\"meal_name\" id=\"meal_name\" class=\"mt-1 focus:ring-sf-green focus:border-sf-green block w-full shadow-sm sm:text-sm border-gray-300 rounded-md\">\n              </div>\n\n              <div class=\"col-span-6 sm:col-span-3\">\n                <label for=\"meal_price\" class=\"block text-sm font-medium text-gray-700\">Price (GHS)</label>\n                <input type=\"number\" name=\"meal_price\" id=\"meal_price\"  class=\"mt-1 focus:ring-sf-green focus:border-sf-green block w-full shadow-sm sm:text-sm border-gray-300 rounded-md\">\n              </div>\n              \n\n              \n          <div class=\"px-4 py-3 bg-gray-50 text-right sm:px-6\">\n            <button type=\"submit\" @click=\"updateMenu\" class=\"inline-flex justify-center py-2 px-4 border border-transparent shadow-sm text-sm font-medium rounded-md text-white bg-sf-green hover:bg-sf-dark-green focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-sf-green\">\n              Save\n            </button>\n          </div>\n          </div>\n          </div>\n          </div>\n      </form>\n    </div>\n    </div>\n</template>\n<script>\nimport db from \"./firebaseInit\"\nexport default {\n    data () {\n        return {\n    \n        mealID: null,\n        mealName: null,\n        mealPrice: null,\n        }\n    },\n    beforeRouteEnter (to, from, next) {\n      db.collection('menus/saladMenu/dessertMenu').where('mealID', '==', to.params.mealID).get().then((querySnapshot) => {\n        querySnapshot.forEach((doc) => {\n          next(vm => {\n            vm.mealID = doc.id\n            vm.mealName = doc.data().name\n            vm.mealPrice = doc.data().price\n          })\n        })\n      })\n    },\n    watch: {\n      '$route': 'fetchData'\n    },\n     methods: {\n      fetchData () {\n        db.collection('menus/saladMenu/dessertMenu').where('mealID', '==', this.$route.params.mealID).get().then((querySnapshot) => {\n          querySnapshot.forEach((doc) => {\n            this.mealID = doc.id\n            this.mealName = doc.data().name\n            this.mealPrice = doc.data().price\n          })\n        })\n      },\n      \n      }\n\n}\n</script>\n"],"sourceRoot":""}]}