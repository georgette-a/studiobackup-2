           </dd>\n            </div>\n    \n            \n            <div class=\"bg-white px-4 py-5 sm:grid sm:grid-cols-3 sm:gap-4 sm:px-6\">\n                <dt class=\"text-sm font-medium text-gray-500\">\n                Ingredients\n                </dt>\n                <dd class=\"mt-1 text-sm text-gray-900 sm:mt-0 sm:col-span-2\">\n\n                <div class=\" col-span-6 flex-wrap\">\n                    <a href=\"#\" class=\"inline-block rounded-full text-white\n                    bg-sf-green hover:bg-sf-dark-green duration-300\n                    text-xs font-bold \n                    mr-1 md:mr-2 mb-2 px-2 md:px-4 py-1 \n                    opacity-90 hover:opacity-100\">\n                    GHS \n                    </a>\n                    </div>\n\n                </dd>\n            </div>\n            \n            </dl>\n        </div>\n        </div>        \n    </div>\n\n\n    </div>\n</template>\n<script>\nimport db from \"./firebaseInit\"\nexport default {\n\n   beforeRouteEnter (to, from, next) {\n      db.collection('menus/saladMenu/saladMeals').where('salad_id', '==', to.params.salad_id).get().then((querySnapshot) => {\n        querySnapshot.forEach((doc) => {\n          next(vm => {\n            vm.salad_id = doc.data().id\n            vm.name = doc.data().name\n            vm.size = doc.data().sizes\n            vm.ingredients = doc.data().ingredients\n          })\n        })\n      })\n    },\n    watch: {\n      '$route': 'fetchData'\n    },\n    methods: {\n      fetchData () {\n        db.collection('salads').where('salad_id', '==', this.$route.params.salad_id).get().then((querySnapshot) => {\n          querySnapshot.forEach((doc) => {\n            this.salad_id = doc.data().id\n            this.name = doc.data().name\n            this.size = doc.data().sizes\n            this.ingredients = doc.data().ingredients\n          })\n        })\n      },\n      \n    }\n    \n}\n</script>\n"]}]}