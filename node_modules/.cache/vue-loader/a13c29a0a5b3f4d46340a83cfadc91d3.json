odel=\"password\">\n      \n      <button type=\"submit\" class=\" bg-st-gray w-40 my-10 mx-20 text-white font-semibold h-14\" >log in</button>\n  </form>\n  <!-- <h1>{{password}}, {{email}}</h1> -->\n  </div>\n  </div>\n</div>\n</template>\n\n<script>\nimport firebase from 'firebase/app'\nimport Nav1 from './Nav3' \nexport default {\n  name: 'App',\n  components: { Nav1 },\n    data(){\n        return{\n            email:'',\n            password:'',\n        }\n    },\n    methods: {\n  login() {\n    firebase\n      .auth()\n      .signInWithEmailAndPassword(this.email, this.password)\n      .then(() => {\n        alert('Successfully logged in');\n        this.$router.push('/dashboard');\n      })\n      .catch(error => {\n        alert(error.message);\n      });\n  },\n}\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n\n\n</style>"]}]}